{
  "name": "@asigloo/vue-dynamic-forms",
  "version": "0.0.0-development",
  "description": "Easy way to dynamically create reactive forms in vue based on varying business object model",
  "author": "Alvaro Saburido <alvaro.saburido@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/alvarosaburido/vue-dynamic-forms.git"
  },
  "bugs": {
    "url": "https://github.com/alvarosaburido/vue-dynamic-forms/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "master",
      "next",
      "next-major",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ]
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name as-dynamic-forms src/index.js",
    "lint": "vue-cli-service lint",
    "test:watch": "vue-cli-service test:unit --verbose --no-cache --watchAll",
    "test": "vue-cli-service test:unit",
    "semantic-release": "semantic-release"
  },
  "main": "dist/as-dynamic-forms.common.js",
  "devDependencies": {
    "@vue/cli-plugin-babel": "4.5.7",
    "@vue/cli-plugin-eslint": "4.5.7",
    "@vue/cli-plugin-unit-jest": "4.5.7",
    "@vue/cli-service": "4.5.7",
    "@vue/eslint-config-prettier": "6.0.0",
    "@vue/test-utils": "1.1.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.5.2",
    "eslint": "7.11.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-vue": "7.0.1",
    "node-sass": "4.14.1",
    "prettier": "2.1.2",
    "sass-loader": "10.0.3",
    "vue-select": "3.10.8",
    "vue-template-compiler": "2.6.12",
    "semantic-release": "17.1.2",
    "bootstrap": "^4.5.2",
    "core-js": "^3.6.5",
    "sass-resources-loader": "^2.1.1",
    "vue": "^2.6.12"
  }
}
