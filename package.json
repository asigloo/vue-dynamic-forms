{
  "name": "@asigloo/vue-dynamic-forms",
  "version": "0.0.0-development",
  "description": "Easy way to dynamically create reactive forms in vue based on varying business object model",
  "author": "Alvaro Saburido <alvaro.saburido@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/asigloo/vue-dynamic-forms.git"
  },
  "bugs": {
    "url": "https://github.com/asigloo/vue-dynamic-forms/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "master",
      "next",
      "next-major",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ]
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "rollup -c rollup.config.js",
    "build:dts": "api-extractor run --local --verbose && tail -n +7 >> dist/as-dynamic-forms.d.ts",
    "lint": "vue-cli-service lint",
    "test:watch": "vue-cli-service test:unit --verbose --no-cache --watchAll",
    "test": "vue-cli-service test:unit",
    "semantic-release": "semantic-release"
  },
  "main": "dist/as-dynamic-forms.cjs.js",
  "browser": "dist/as-dynamic-forms.esm.js",
  "unpkg": "dist/as-dynamic-forms.global.js",
  "jsdelivr": "dist/as-dynamic-forms.global.js",
  "module": "dist/as-dynamic-forms.esm-bundler.js",
  "types": "dist/as-dynamic-forms.d.ts",
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.10.1",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "@vue/cli-plugin-babel": "4.5.6",
    "@vue/cli-plugin-eslint": "4.5.6",
    "@vue/cli-plugin-typescript": "~4.5.6",
    "@vue/cli-plugin-unit-jest": "4.5.6",
    "@vue/cli-service": "~4.5.6",
    "@vue/compiler-sfc": "3.0.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.1.0",
    "@vue/test-utils": "1.1.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.5.0",
    "chalk": "^4.1.0",
    "eslint": "7.10.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^7.0.1",
    "node-sass": "4.14.1",
    "prettier": "2.1.2",
    "rollup": "^2.28.2",
    "rollup-plugin-analyzer": "^3.3.0",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-scss": "^2.6.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.27.3",
    "rollup-plugin-vue": "^6.0.0-beta.10",
    "sass-loader": "10.0.2",
    "semantic-release": "17.1.2",
    "tailwindcss": "^1.8.10",
    "ts-node": "^9.0.0",
    "typescript": "~4.0.3",
    "vue": "^3.0.0",
    "vue-select": "3.10.8"
  },
  "dependencies": {
    "deep-clone": "^3.0.3",
    "deep-object-diff": "^1.1.0",
    "rollup-plugin-scss": "^2.6.1"
  }
}
